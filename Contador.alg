algoritmo "Contador"
var
   Valor, Contador : Inteiro
inicio
      Contador <- 0
      Escreva ("Quer contar ate quanto ?")
      leia (valor)
      enquanto (Contador <= Valor) faca
               Escreval (Contador)
               Contador <- Contador +10
      Fimenquanto
      Escreva ("Terminei de contar")
fimalgoritmo
